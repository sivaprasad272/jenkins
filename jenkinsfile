pipeline {
  agent any
  stages {
    stage('Clone Git') {
      steps {
        git(url: 'https://github.com/sivaprasad272/jenkins.git')
      }
    }
    stage('Build Image') {
      steps {
        script {
          dockerImage = docker.build('githububuntu')
        }
      }
    }
    stage('Push Image') {
      steps {
        script {
          docker.withRegistry('https://registry.hub.docker.com', 'your_docker_hub_credentials') {
            dockerImage.push()
          }
        }
      }
    }
  }
}
